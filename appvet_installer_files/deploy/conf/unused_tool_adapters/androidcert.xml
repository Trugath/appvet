<?xml version="1.0" encoding="UTF-8"?>
<appvet:ToolServiceAdapter xmlns:appvet="http://csrc.nist.gov/projects/appvet" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://csrc.nist.gov/projects/appvet ToolServiceAdapter.xsd ">
  <appvet:Description>
    <appvet:Name>Android Cert Verifier</appvet:Name>
    <appvet:Id>certverify</appvet:Id>
    <appvet:VendorName>NIST</appvet:VendorName>
    <appvet:VendorWebsite>http://csrc.nist.gov/projects/appvet</appvet:VendorWebsite>
    <appvet:ReportFile>TXT</appvet:ReportFile>
  </appvet:Description>
  <appvet:Protocol>
    <appvet:Type>SYNCHRONOUS</appvet:Type>
    <appvet:Synchronous>
      <appvet:Request>
        <appvet:URL>https://appsec.nist.gov/android-cert-test/SynchronousService</appvet:URL>
        <appvet:Method>POST</appvet:Method>
        <appvet:Parameter>
          <appvet:Name>file</appvet:Name>
          <appvet:Value>APP_FILE</appvet:Value>
        </appvet:Parameter>
      </appvet:Request>
      <appvet:Response>
        <appvet:AppVetRiskHeaderName>toolrisk
        </appvet:AppVetRiskHeaderName>
        <appvet:ReportPayload>true</appvet:ReportPayload>
      </appvet:Response>
    </appvet:Synchronous>
  </appvet:Protocol>
</appvet:ToolServiceAdapter>